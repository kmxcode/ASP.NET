@model IEnumerable<string>

<a class="btn btn-block btn-secondary"
   asp-action="List"
   asp-controller="Product"
   asp-route-category="">
    Strona głowna
</a>

@foreach (string category in Model)
{
    <a class="btn btn-block btn-secondary"
        asp-action="List"
        asp-controller="Product"
        asp-route-category="@category">
        @category
    </a>
}